<h1>Student Dashboard</h1>

<!-- High School Student Section -->
<div class="panel panel-primary">
  <div class="panel-heading">
    <h3 class="panel-title">High School Students</h3>
  </div>
  <div class="panel-body">
    <!-- Assignment Upload -->
    <h4>Assignment Upload</h4>
    <form action="#" method="post" enctype="multipart/form-data">
      <div class="form-group">
        <label for="assignment">Choose File:</label>
        <input type="file" id="assignment" name="assignment" />
      </div>
      <div class="form-group">
        <label for="expected-date">Expected Date:</label>
        <input type="date" id="expected-date" name="expected-date" />
      </div>
      <button type="submit" class="btn btn-primary">Upload</button>
    </form>

    <!-- Assignment List -->
    <h4>Submitted Assignments</h4>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Assignment Name</th>
          <th>Date Submitted</th>
          <th>Expected Date</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>Assignment 1</td>
          <td>June 15, 2023</td>
          <td>June 14, 2023</td>
          <td>Received</td>
        </tr>
        <tr>
          <td>Assignment 2</td>
          <td>June 16, 2023</td>
          <td>June 15, 2023</td>
          <td>Received</td>
        </tr>
        <tr>
          <td>Assignment 3</td>
          <td>June 17, 2023</td>
          <td>June 16, 2023</td>
          <td>Not Received</td>
        </tr>
      </tbody>
    </table>
  </div>
</div>

